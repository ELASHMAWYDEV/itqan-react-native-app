{
  "expo": {
    "name": "itqan",
    "slug": "itqan",
    "scheme": "itqan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/main-img/icon.png",
    "splash": {
      "image": "./app/assets/main-img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3E47C7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "itqan"
    },
    "web": {
      "favicon": "./app/assets/main-img/favicon.png"
    },
    "description": "This is the development build for itqan e-learning app for ios and android",
    "android": {
      "package": "com.itqan.elearning"
    },
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
